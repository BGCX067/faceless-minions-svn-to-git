		<background:encounter_skillbar.png/>
		<portrait>
			<pos_x:200/>
			<pos_y:25/>
		</portrait>
		<default_icon:icon_empty.png/>
		<icon_size>
			<width:91/>
			<height:80/>
		</icon_size>
		<skill>
			<pos_x:300/>
			<pos_y:40/>
		</skill>
		<skill>
			<pos_x:366/>
			<pos_y:0/>
		</skill>
		<skill>
			<pos_x:433/>
			<pos_y:40/>
		</skill>
		<skill>
			<pos_x:499/>
			<pos_y:0/>
		</skill>
		<skill>
			<pos_x:566/>
			<pos_y:40/>
		</skill>
	</skill_bar>
	<icons>
		<mage_base_attack:icon_sword.png/>
		<quicksilver_base_attack:icon_sword.png/>
		<quicksilver_rooted_dagger:icon_rootingdagger.png/>
		<ironclad_seismic_slam:icon_groundslam.png/>
		<ironclad_base_attack:icon_sword.png/>
		<mage_rising_shards:icon_risingshards.png/>
		<hero_fireball:icon_fireball.png/>
		<dummy_ability:icon_empty.png/>
	</icons>
	<menu_buttons>
		<new>
			<sprite:images/interface/icons/icon_sword.png/>
			<label:New Game/>
		</new>
		<continue>
			<sprite:images/interface/icons/icon_fireball.png/>
			<label:Continue/>
		</continue>
		<options>
			<sprite:images/interface/icons/icon_risingshards.png/>
			<label:Options/>
		</options>
		<quit>
			<sprite:images/interface/icons/icon_empty.png/>
			<label:Quit/>
		</quit>
		
		#for debugging
		<encounter>
			<sprite:images/interface/icons/icon_fireball.png/>
			<label:Debug Encounter/>
		</encounter>
	</menu_buttons>
	<main_menu>
		<background:images/menu/background.png/>
		<music:menu/>
		<button:new>
			<target:3/> #enum new
			<pos_x:950/>
			<pos_y:50/>
		</button>
		<button:continue>
			<target:7/> #enum debug
			<pos_x:1000/>
			<pos_y:150/>
		</button>
		<button:options>
			<target:5/> #enum options
			<pos_x:950/>
			<pos_y:250/>
		</button>
		<button:quit>
			<target:6/> #enum quit
			<pos_x:1000/>
			<pos_y:350/>
		</button>
	</main_menu>
	<encounter_dialog>
		<background:images/interface/dialog_background.png/>
	</encounter_dialog>
	<post_battle_dialog>
		<background:images/interface/post_battle_background.png/>
	</post_battle_dialog>
	<overworld_menu>
		<background:images/overworld/gui/menu_bg.png/>
		<gold_icon:images/overworld/gui/gold_icon.png/>
