<ui>
	<cursor>
		<sprite_sheet:cursor/>
	</cursor>
	<fonts>
		<text:resources/fonts/dayroman.ttf/>
		<numbers:resources/fonts/oldstyle.ttf/>
		<console:resources/fonts/SVBasicManual.ttf/>
	</fonts>
	<shaders>
		<color_overlay:resources/shaders/color_overlay.frag/>
		<grayscale_luminosity:resources/shaders/grayscale_lumin.frag/>
		<color_mask:resources/shaders/color_mask.frag/>
		<outline:resources/shaders/outline.frag/>
		<faded_outline:resources/shaders/faded_outline.frag/>
	</shaders>
	<grid>
		<grid_tile:grid_tile/>
	</grid>
	<skill_bar>
		<position_x:0/>
