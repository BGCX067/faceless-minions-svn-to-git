	<options>
		<slider>
			<bar_graphic:images/ui/slider_bar.png/>
			<slider_graphic:images/ui/slider_knob.png/>
			<min_offset:10/>
			<max_offset:290/>
		</slider>
		<checkbox>
			<sprite_sheet:checkbox/>
			<sprite_name:checkbox/>
