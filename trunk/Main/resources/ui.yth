<ui>
	<cursor>
		<sprite_sheet:cursor/>
	</cursor>
	<fonts>
		<text:resources/fonts/dayroman.ttf/>
		<numbers:resources/fonts/oldstyle.ttf/>
		<console:resources/fonts/SVBasicManual.ttf/>
	</fonts>
	<shaders>
		<color_overlay:resources/shaders/color_overlay.frag/>
		<grayscale_luminosity:resources/shaders/grayscale_lumin.frag/>
		<color_mask:resources/shaders/color_mask.frag/>
		<outline:resources/shaders/outline.frag/>
		<faded_outline:resources/shaders/faded_outline.frag/>
	</shaders>
	<grid>
		<grid_tile:grid_tile/>
	</grid>
	<skill_bar>
		<position_x:0/>
		<position_y:600/>
		<background:encounter_skillbar.png/>
		<portrait>
			<pos_x:200/>
			<pos_y:25/>
		</portrait>
		<default_icon:icon_empty.png/>
		<icon_size>
			<width:91/>
			<height:80/>
		</icon_size>
		<skill>
			<pos_x:300/>
			<pos_y:40/>
		</skill>
		<skill>
			<pos_x:366/>
			<pos_y:0/>
		</skill>
		<skill>
			<pos_x:433/>
			<pos_y:40/>
		</skill>
		<skill>
			<pos_x:499/>
			<pos_y:0/>
		</skill>
		<skill>
			<pos_x:566/>
			<pos_y:40/>
		</skill>
	</skill_bar>
	<icons>
		<quicksilver_base_attack:icon_sword.png/>
		<quicksilver_rooted_dagger:icon_rootingdagger.png/>
		<quicksilver_riposte:icon_sword.png/>
		<quicksilver_blink_cloak:icon_sword.png/>
		<ironclad_seismic_slam:icon_groundslam.png/>
		<ironclad_base_attack:icon_sword.png/>
		<ironclad_berzerker:icon_sword.png/>
		<ironclad_cascading_pummels:icon_sword.png/>
		<ironclad_whirlwind:icon_sword.png/>
		<mage_base_attack:icon_sword.png/>
		<mage_rising_shards:icon_risingshards.png/>
		<mage_shattered_journey:icon_fireball.png/>
		<mage_shardpunch:icon_fireball.png/>
		<mage_overload:icon_fireball.png/>
		<hero_fireball:icon_fireball.png/>
		<dummy_ability:icon_empty.png/>
	</icons>
	<menu_buttons>
		<new>
			<sprite:images/ui/icons/icon_sword.png/>
			<label:New Game/>
		</new>
		<continue>
			<sprite:images/ui/icons/icon_fireball.png/>
			<label:Continue/>
		</continue>
		<options>
			<sprite:images/ui/icons/icon_risingshards.png/>
			<label:Options/>
		</options>
		<quit>
			<sprite:images/ui/icons/icon_empty.png/>
			<label:Quit/>
		</quit>
		
		#for debugging
		<encounter>
			<sprite:images/ui/icons/icon_fireball.png/>
			<label:Debug Encounter/>
		</encounter>
	</menu_buttons>
	<main_menu>
		<background:images/menu/background.png/>
		<music:menu/>
		<button:new>
			<target:3/> #enum new
			<enabled:true/>
			<pos_x:950/>
			<pos_y:50/>
		</button>
		<button:continue>
			<target:4/> #enum continue
			<enabled:true/>
			<pos_x:1000/>
			<pos_y:150/>
		</button>
		<button:options>
			<target:5/> #enum options
			<enabled:true/>
			<pos_x:950/>
			<pos_y:250/>
		</button>
		<button:quit>
			<target:6/> #enum quit
			<enabled:true/>
			<pos_x:1000/>
			<pos_y:350/>
		</button>
	</main_menu>
	<encounter_dialog>
		<background:images/ui/dialog_background.png/>
	</encounter_dialog>
	<post_battle_dialog>
		<background:images/ui/post_battle_background.png/>
	</post_battle_dialog>
	<overworld_menu>
		<background_left:images/overworld/gui/menu_bar_left.png/>
		<background_right:images/overworld/gui/menu_bar_right.png/>
		<gold_icon:images/overworld/gui/gold_icon.png/>
		<button_graphic:images/overworld/gui/menu_button.png/>
		<party_button:Party/>
		<stats_button:Inventory/>
		<menu_button:Options/>
	</overworld_menu>
